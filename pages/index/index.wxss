/**index.wxss**/

.index {
  width: 100%;
  height: auto;
}

.index-top {
  width: 100%;
  height: 370rpx;
  position: relative;
}

.index-top-image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.index-top-image>image {
  width: 100%;
  height: 100%;
}

.index-top-wz {
  width: 702rpx;
  height: auto;
  padding: 0 24rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
}

.index-top-wz>view:first-child {
  font-size: 48rpx;
  color: #fff;
  margin-top: 80rpx;
  margin-bottom: 20rpx;
  font-weight: 600;
}

.index-top-wz>view:nth-child(2) {
  font-size: 28rpx;
  color: #fff;
}

.index-sou {
  width: 100%;
  height: 76rpx;
  line-height: 76rpx;
  display: flex;
  margin-top: 60rpx;
}

.index-sou>input {
  flex: 1;
  height: 100%;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  padding: 0 20rpx;
  border-top-left-radius: 15rpx;
  border-bottom-left-radius: 15rpx;
}

.index-sou>view {
  width: 150rpx;
  height: 100%;
  text-align: center;
  background: #fe6306;
  border-top-right-radius: 15rpx;
  border-bottom-right-radius: 15rpx;
}

.index-sou>view>image {
  width: 40rpx;
  height: 40rpx;
  padding-top: 18rpx;
}


/*公告*/

.gong {
  width: 702rpx;
  height: 96rpx;
  line-height: 96rpx;
  margin: 20rpx 24rpx 0 24rpx;
  display: flex;
  background: #fff;
  border-radius: 10rpx;
}

.gong-left {
  width: 60rpx;
  height: 60rpx;
  padding: 18rpx 20rpx;
}

.gong-left>image {
  width: 100%;
  height: 100%;
}

.gong-right {
  flex: 1;
  padding: 14rpx 20rpx 10rpx 0;
  overflow: hidden;
}

.swiper_container {
  width: 100%;
  height: 72rpx;
}

.swiper_item {
  width: 100%;
  height: 37rpx !important;
  line-height: 37rpx;
}

.swiper_item1 {
  width: 100%;
  line-height: 37rpx;
  font-size: 24rpx;
  color: #444;
  overflow: hidden;
  word-break: keep-all;
  white-space: nowrap;
  text-overflow: ellipsis;
}


/*精品商城*/

.shang {
  width: 702rpx;
  height: 220rpx;
  margin: 20rpx 24rpx 0 24rpx;
  background: #fff;
  display: flex;
}

.shang-item {
  flex: 1;
  text-align: center;
  border-right: 1px solid #e6e6e6;
}

.shang-item:last-child {
  border-right: 0;
}

.shang-item>image {
  width: 100rpx;
  height: 100rpx;
  margin-top: 40rpx;
}

.shang-item>view {
  font-size: 28rpx;
  color: #333;
  line-height: 46rpx;
}

.xiu {
  width: 702rpx;
  height: 310rpx;
  margin: 20rpx 24rpx 0 24rpx;
  border-radius: 10rpx;
  background: #fff;
}

.xiu-top {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px dashed #ccc;
}

.xiu-top>view:first-child {
  font-size: 28rpx;
  color: #333;
  border-left: 5rpx solid #fe6306;
  padding-left: 15rpx;
}

.xiu-top>view:last-child {
  display: flex;
  padding-right: 20rpx;
  font-size: 24rpx;
  color: #df273f;
}

.xiu-top>view:last-child>text {
  display: block;
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #fe6306;
  margin: 10rpx;
}

.xiu-bottom {
  width: 662rpx;
  height: auto;
  margin: 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
}

.xiu-b-left {
  width: 180rpx;
  height: 180rpx;
  margin-right: 30rpx;
  position: relative;
}

.xiu-b-left>image {
  width: 100%;
  height: 100%;
  box-shadow: 0 0 5rpx #f0f0f0;
}

.xiu-b-left>view {
  width: 120rpx;
  height: 46rpx;
  line-height: 46rpx;
  padding: 0 15rpx;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  background: #ff9a44;
  font-size: 24rpx;
  color: #fff;
  border-bottom-right-radius: 30rpx;
  overflow: hidden;
  word-break: keep-all;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.xiu-right {
  flex: 1;
}

.xiu-right>view:first-child {
  width: 100%;
  height: 68rpx;
  line-height: 34rpx;
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.xiu-right>view:nth-child(2) {
  margin-top: 40rpx;
}

.xiu-right>view {
  font-size: 24rpx;
  color: #666;
  line-height: 38rpx;
}

